{
	"name": "vuejs-parcel-starter",
	"description": "VueJs Parcel Starter",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "parcel build ./src/index.html --no-content-hash",
		"prebuild:prod": "rimraf dist",
		"build:prod": "parcel build ./src/index.html --no-content-hash --no-source-maps",
		"start": "parcel ./src/index.html --port 8080 --open"
	},
	"dependencies": {
		"vue": "3.2.47",
		"vue-hot-reload-api": "2.3.4"
	},
	"devDependencies": {
		"@parcel/transformer-vue": "^2.8.3",
		"@vue/component-compiler-utils": "3.3.0",
		"cssnano": "6.0.0",
		"rimraf": "^5.0.0",
		"sass": "1.62.0",
		"vue-template-compiler": "2.7.14"
	}
}
